[alias]
    co = checkout
    br = branch
    ci = commit
    st = status
    unstage = reset HEAD --
    last = log -2 HEAD -p
    hide = update-index --assume-unchanged
    unhide = update-index --no-assume-unchanged

[user]
    name = ClaudeSeo
    email = ehdaudtj@gmail.com

[push]
    autoSetupRemote = true

[core]
    editor = nvim

[push]
    default = current

[pull]
    rebase = true
